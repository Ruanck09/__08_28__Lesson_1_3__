   "source": [
    "# 高中數學\n",
    "# 輸入直角三角形的斜邊及對邊，計算對邊所對的角度\n",
    "\n",
    "import math\n",
    "\n",
    "a = float(input(\"請輸入直角三角形的對邊：\"))\n",
    "b = float(input(\"請輸入直角三角形的斜邊：\"))\n",
    "radian = math.asin(a / b)\n",
    "degree = math.degrees(radian)\n",
    "\n",
    "print(f\"對邊：{a},斜邊：{b}\")\n",
    "print(f\"弳度是{radian:.3f},角度是{round(degree)}\")"
   ]
